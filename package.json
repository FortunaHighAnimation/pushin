{
  "name": "pushin",
  "description": "A javascript plugin to attach a 'dolly' or 'push-in' effect to a div element when user scrolls.",
  "version": "3.4.1",
  "main": "dist/pushin.min.js",
  "scripts": {
    "husky": "husky",
    "lint-staged": "lint-staged",
    "build": "webpack",
    "docs": "webpack --name=docs",
    "start": "webpack serve --name=docs --mode=development",
    "test": "mocha --watch",
    "coverage": "nyc --reporter=html --reporter=text mocha && open coverage/index.html -a 'Google Chrome'",
    "test:once": "mocha",
    "postinstall": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nateplusplus/pushIn-js.git"
  },
  "keywords": [
    "parallax",
    "zoom",
    "push-in",
    "movement",
    "motion",
    "effect"
  ],
  "author": "Nathan Blair <nate@natehub.net> (https://natehub.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nateplusplus/pushIn-js/issues"
  },
  "homepage": "http://nateplusplus.github.io/pushIn-js/",
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "babel-loader": "^8.2.3",
    "chai": "^4.3.4",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.3.7",
    "mini-css-extract-plugin": "^2.5.2",
    "mocha": "^9.1.4",
    "nyc": "^15.1.0",
    "path": "^0.12.7",
    "prettier": "^2.6.1",
    "pug": "^3.0.2",
    "pug-loader": "^2.4.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.0",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3"
  },
  "prettier": {
    "endOfLine": "lf",
    "singleQuote": true,
    "arrowParens": "avoid",
    "bracketSpacing": true
  },
  "lint-staged": {
    "*.{js,css,md}": ["prettier --write"]
  }
}
